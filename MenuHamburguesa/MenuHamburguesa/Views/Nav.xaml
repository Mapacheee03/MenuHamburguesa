<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MenuHamburguesa.Nav"
             Title="Nav">
    <Grid BackgroundColor="#29af66">
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".5*"/>
            <ColumnDefinition Width=".5*"/>
        </Grid.ColumnDefinitions>
        
        <ImageButton BackgroundColor="Transparent" x:Name="inicio" Source="inicio.png" Grid.Column="0" Grid.Row="0" Clicked="GoInicio"/>
        <ImageButton BackgroundColor="Transparent" x:Name="monitoreo" Source="monitoreo.png" Grid.Column="0" Grid.Row="1" Clicked="GoMonitoreo"/>
        <ImageButton BackgroundColor="Transparent" x:Name="reigstro" Source="registro.png" Grid.Column="0" Grid.Row="2" Clicked="GoRegistro"/>
        <ImageButton BackgroundColor="Transparent" x:Name="configuracion" Source="configuraciones.png" Grid.Column="0" Grid.Row="3" Clicked="configuracion_Clicked"/>
        <ImageButton BackgroundColor="Transparent" x:Name="cerrarsesior" Source="cerrarsesion.png" Grid.Column="0" Grid.Row="4"/>
   

    </Grid>
</ContentPage>